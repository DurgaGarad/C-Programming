//formula of triangle

#include <stdio.h>

int main()
{
	int a,b,c;
	printf("Enter the frist number");
	scanf("%d",&a);
    printf("Enter the second number");
	scanf("%d",&b);
	printf("Enter the thrid number");
	scanf("%d",&c);

	int triangle=a+b+c;
	printf("triangle is= %d",triangle);


	
	return 0;
}
