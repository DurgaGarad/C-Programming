//Perimeter of right triangle  p=b+h+d 

#include <stdio.h>

int main()
{
	int b,h,d,p;
	printf("Enter the value of base : ");
	scanf("%d",&b);
    printf("Enter the value of height : ");
	scanf("%d",&h);
	printf("Enter the value of d : ");
	scanf("%d",&d);

	p=b+h+d;
	printf("perimeter right triangle is =%d",p);
	return 0;
}
