#include <stdio.h>
int main()
{
	float r,h,area,volume;
	float PI=3.14;
	printf("Enter the radius & height");
	scanf("%f%f",&r,&h);
	area=(2*PI*r*r)+(2*PI*r*h);
	printf("Enter the surface area of cylinder is =%f \n",area);
	volume=PI*r*r*h;
	printf("the volume of cylinder is =%f\n",volume);
	return 0;
}
